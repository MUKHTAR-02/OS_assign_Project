#include <iostream>
using namespace std;
int main()
{

    int numProcess, process, arrivalTime, burstTime;

    cout << "                   <-+-+-+-+-+-+-+__WELCOME__+-+-+-+-+-+-+->            " << endl;
    cout << endl;

    cout << "Here you can find suitable CPU scheduling methods among these :- " << endl;
    cout << endl;
    cout << " 1) Round cabin scheduling " << endl;
    cout << " 2) First come first serve (FCFS) " << endl;
    cout << " 3) Shortest job first (SJF - preemptive) " << endl;
    cout << " 4) Shortest job first (SJF - non-preemptive ) " << endl;

    cout << " Enter number of processes : " << endl;
    cin >> numProcess;

    return 0;
}
